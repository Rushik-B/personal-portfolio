    style={{ \n              width: '130px', \n              height: '175px',\n              transform: 'rotate(50deg)',\n              transformOrigin: 'center',\n              transition: 'transform 0.3s ease-in-out',\n              ':hover': {\n                transform: 'rotate(55deg)' // adds 5 degrees on hover\n              }\n            }}\n            />  \n          </Navbar.Brand>\n          <Navbar.Toggle aria-controls=\"basic-navbar-nav\">\n            <span className=\"navbar-toggler-icon\"></span>\n          </Navbar.Toggle>\n          <Navbar.Collapse id=\"basic-navbar-nav\">\n            <Nav className=\"ms-auto\">\n              <Nav.Link href=\"#home\" className={activeLink === 'home' ? 'active navbar-link' : 'navbar-link'} onClick={() => onUpdateActiveLink('home')}>Home</Nav.Link>\n              <Nav.Link href=\"#projects\" className={activeLink === 'projects' ? 'active navbar-link' : 'navbar-link'} onClick={() => onUpdateActiveLink('projects')}>Projects</Nav.Link>\n              <Nav.Link \n                href={ResumePlaceholder} \n                target=\"_blank\" \n                rel=\"noopener noreferrer\" \n                className={`nav-link navbar-link ${activeLink === 'resume' ? 'active' : ''}`} \n                onClick={() => onUpdateActiveLink('resume')}\n              >\n                Resume\n              </Nav.Link>\n            </Nav>\n            <span className=\"navbar-text\">\n              <div className=\"social-icon\">\n                <a \n                  href=\"https://www.linkedin.com/in/rushik-behal/\" \n                  target=\"_blank\" \n                  rel=\"noopener noreferrer\"\n                >\n                  <img src={navIcon1} alt=\"LinkedIn\" />\n                </a>\n                <a \n                  href=\"https://github.com/Rushik-B\" \n                  target=\"_blank\" \n                  rel=\"noopener noreferrer\"\n                >\n                  <img src={navIcon2} alt=\"GitHub\" />\n                </a>\n                <a \n                  href=\"https://www.x.com\" \n                  target=\"_blank\" \n                  rel=\"noopener noreferrer\"\n                >\n                  <img src={navIcon3} alt=\"Instagram\" />\n                </a>\n              </div>\n              <HashLink to='#connect'>\n                <button className=\"vvd\"><span>Hire Me!</span></button>\n              </HashLink>\n            </span>\n          </Navbar.Collapse>\n        </Container>\n      </Navbar>\n    </Router>\n  )\n}\n","/Users/Rushik/Documents/personal-portfolio/src/components/BackgroundLayout.js",[],[],"/Users/Rushik/Documents/personal-portfolio/src/components/Contact.js",[],[],"/Users/Rushik/Documents/personal-portfolio/src/components/ProjectCard.js",["131"],[],"import { Col } from \"react-bootstrap\";\n\nexport const ProjectCard = ({ title, description, imgUrl }) => {\n  return (\n    <Col size={12} sm={6} md={4}>\n      <div className=\"proj-imgbx\">\n        <img src={imgUrl} />\n        <div className=\"proj-txtx\">\n          <h4>{title}</h4>\n          <span>{description}</span>\n        </div>\n      </div>\n    </Col>\n  )\n}\n",[],{"ruleId":"132","severity":1,"message":"133","line":1,"column":8,"nodeType":"134","messageId":"135","endLine":1,"endColumn":12},{"ruleId":"132","severity":1,"message":"136","line":12,"column":10,"nodeType":"134","messageId":"135","endLine":12,"endColumn":15},{"ruleId":"132","severity":1,"message":"137","line":22,"column":9,"nodeType":"134","messageId":"135","endLine":22,"endColumn":15},{"ruleId":"138","severity":1,"message":"139","line":32,"column":6,"nodeType":"140","endLine":32,"endColumn":12,"suggestions":"141"},{"ruleId":"142","severity":1,"message":"143","line":17,"column":15,"nodeType":"144","endLine":17,"endColumn":27},{"ruleId":"142","severity":1,"message":"143","line":18,"column":15,"nodeType":"144","endLine":18,"endColumn":27},{"ruleId":"142","severity":1,"message":"143","line":19,"